{
  "name": "react-postcss-boilerplate",
  "version": "1.0.0",
  "description": "A React boilerplate using PostCSS-cssnext",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --open",
    "build": "NODE_ENV='production' webpack -p",
    "deploy": "npm run build && firebase deploy",
    "firebase-init": "firebase login && firebase init",
    "test": "mocha --opts ./mocha.options ./app/**/*.test.js",
    "test:watch": "npm run test -- -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tyreer/react-postcss-boilerplate.git"
  },
  "author": "Robert Tyree",
  "license": "ISC",
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "compact": true
  },
  "bugs": {
    "url": "https://github.com/tyreer/react-postcss-boilerplate/issues"
  },
  "homepage": "https://github.com/tyreer/react-postcss-boilerplate#readme",
  "dependencies": {
    "axios": "^0.16.1",
    "prop-types": "^15.5.8",
    "react": "^16.2.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "caniuse-lite": "^1.0.30000780",
    "chai": "^4.1.2",
    "chai-enzyme": "^1.0.0-beta.0",
